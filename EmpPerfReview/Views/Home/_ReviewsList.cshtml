<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
            <th>Detail</th>
            <th>Employee</th>
        </tr>
    </thead>

    <tbody data-bind="foreach: reviews">
        <tr>
            <td data-bind="text: Detail"></td>
            <td data-bind="text: EmployeeId"></td>
        </tr>
    </tbody>
</table>

<br />

<input type="button" class="btn btn-default" id="btnGetReviews" value="Refresh" data-bind="click: getReviews" />
